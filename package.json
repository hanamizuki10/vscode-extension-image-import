{
  "name": "vscode-image-import",
  "displayName": "vscode-image-import",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "views": {
			"explorer": [
				{
					"type": "webview",
					"id": "viewExplorerMyLoveCat",
					"name": "My Love Cat"
				}
			]
		},
    "menus": {
      "view/title": [
          {
              "command": "vscode-image-import.viewMyLoveCatRandom",
              "when": "view == viewExplorerMyLoveCat",
              "group": "navigation@1"
          }
      ]
    },
    "commands": [
      {
        "command": "vscode-image-import.viewExplorerMyLoveCat",
        "title": "Explorer View My Love Cat "
      },
      {
        "command": "vscode-image-import.viewPanelMyLoveCat",
        "title": "Panel View My Love Cat"
      },
      {
        "command": "vscode-image-import.viewMyLoveCatRandom",
        "title": "View My Love Cat Random Update",
        "icon": {
          "dark": "media/icon/dark-reload.svg",
          "light": "media/icon/light-reload.svg"
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.82.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "ts-loader": "^9.4.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1",
    "@vscode/test-electron": "^2.3.2"
  }
}
